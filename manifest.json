{
	"name": "Tabulater",
	"description": "Manage your tabs!",
	"version": "1.0",
	"manifest_version": 2,
	"permissions": ["tabs"],
	"browser_action": {
	"default_popup": "popup.html",
    "default_title": "Summarize all tabs!",
		"default_icon": {
			"128": "/images/icon.png"
		}
	},
	"icons": {
		"128": "/images/icon.png"
  }
}
