{
	"name": "Tabulater",
	"description": "Manage your tabs!",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": ["storage", "activeTab", "scripting", "tabs"],
	"action": {
    "default_title": "Summarize all tabs!",
		"default_popup": "popup.html",
		"default_icon": {
			"128": "/images/icon.png"
		}
	},
	"icons": {
		"128": "/images/icon.png"
	}
}
